{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "mint-fns",
  "author": "Ryan Macdonald",
  "module": "dist/mint-fns.esm.js",
  "size-limit": [
    {
      "path": "dist/mint-fns.cjs.production.min.js",
      "limit": "14 KB"
    },
    {
      "path": "dist/mint-fns.esm.js",
      "limit": "14 KB"
    }
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest/setup.ts"
    ]
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "7.0.5",
    "@size-limit/webpack": "7.0.5",
    "@size-limit/webpack-why": "7.0.5",
    "husky": "7.0.4",
    "size-limit": "7.0.5",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "bigdecimal.js": "^1.1.0"
  }
}
